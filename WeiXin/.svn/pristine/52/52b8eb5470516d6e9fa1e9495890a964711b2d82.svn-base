package com.ys.weixin.entity;

public class User {

	private int ID;
	private String Name;
	private String WeiXinID;
	private int Role;

	public User() {
		
	}

	public User(String name, String weiXinID, int role) {
		Name = name;
		WeiXinID = weiXinID;
		Role = role;
	}

	public int getID() {
		return ID;
	}

	public void setID(int iD) {
		ID = iD;
	}

	public String getName() {
		return Name;
	}

	public void setName(String name) {
		Name = name;
	}

	public String getWeiXinID() {
		return WeiXinID;
	}

	public void setWeiXinID(String weiXinID) {
		WeiXinID = weiXinID;
	}

	public int getRole() {
		return Role;
	}

	public void setRole(int role) {
		Role = role;
	}

	@Override
	public String toString() {
		return "User [ID=" + ID + ", Name=" + Name + ", WeiXinID=" + WeiXinID
				+ ", Role=" + Role + "]";
	}

}
