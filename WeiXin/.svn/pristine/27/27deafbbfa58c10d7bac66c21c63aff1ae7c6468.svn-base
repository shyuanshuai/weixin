import com.ys.weixin.dao.impl.UserDaoImpl;
import com.ys.weixin.entity.User;

public class UserService {
	
	private static UserDao dao = new UserDaoImpl();
	
	/**
	 * 新增用户
	 * 
	 * @param user
	 * @return
	 */
