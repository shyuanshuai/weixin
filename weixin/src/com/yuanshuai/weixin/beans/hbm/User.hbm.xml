<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.yuanshuai.weixin.beans">

	<class name="User" table="User">

		<id name="id" column="id">
			<generator class="assigned"></generator>
		</id>

		<property name="name" column="name" type="java.lang.String"
			length="255"></property>
		<property name="openId" column="openId" type="java.lang.String"
			length="255"></property>
		<property name="robot" column="robot" type="java.lang.Boolean"></property>
		<property name="addTime" column="addTime" type="java.lang.Long"></property>
		<property name="note" column="note" type="java.lang.String"
			length="255"></property>

	</class>

</hibernate-mapping>